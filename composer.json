{
  "name": "skylarkltd/laravel-freesend",
  "description": "Laravel mail driver for the Freesend email API",
  "keywords": [
    "laravel",
    "mail",
    "email",
    "freesend"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Aaron Carr"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/mail": "^10.0|^11.0|^12.0",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.6",
    "orchestra/testbench": "^8.0|^9.0|^10.0",
    "pestphp/pest": "^2.0|^3.0",
    "pestphp/pest-plugin-laravel": "^2.0|^3.0",
    "phpunit/phpunit": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Skylarkltd\\Freesend\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Skylarkltd\\Freesend\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Skylarkltd\\Freesend\\FreesendServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
